<%- include("partials/header") -%>
<h1>Home</h1>

<p><%= homeContent %></p>

<% posts.forEach((post) => { %>
    <div class="post">
        <h2><%= post.title %></h2>
        <p>
            <%= post.content.substr(0, 100) + "..." %>
            <a href="/posts/<%= post.title %>">Read more</a>
            <% console.log(post.title) %>
        </p>
    </div>
<% }) %>

<%- include("partials/footer") -%>
